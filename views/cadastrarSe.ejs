!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>PETSHOP DH</title>
    
    <link rel="stylesheet" href="stylesheets/style.css">
    <link rel="stylesheet" href="stylesheets/header.css">
    <link rel="stylesheet" href="stylesheets/login.css">


    
</head>

<body>
    <%- include('./partials/header') %>
    

    <main>
        <form action="/login/cadastrar" method="post">
            <label for="nome">Nome</label>
            <input type="text" name="nomeCadastro" id="nome">
            <label for="email">Email</label>
            <input type="email" name="emailCadastro" id="email" placeholder="Exemplo@email.com">
            <label for="senha">Senha</label>
            <input type="password" name="senhaCadastro" id="senha">
            

            <button type="submit">Registrar</button>
        </form>

        
    </main>
    <%- include('./partials/footer') %>

</body>